# name: test_include_with_arguments
  '''
  query {
    result: findUniqueUser
    (
      where: {
        id: 1
      }
    )
    {
      id
      name
      email
      created_at
      posts(
        where: {
          id: 1
        }
      )
      {
        id
        created_at
        updated_at
        title
        published
        views
        desc
        author_id
      }
    }
  }
  '''
