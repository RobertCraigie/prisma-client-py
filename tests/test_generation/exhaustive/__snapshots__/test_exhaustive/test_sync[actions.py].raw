        resp = self._client._execute(
            operation='query',
            method='aggregate',
            model='M',
            arguments={
                'take': take,
                'skip': skip,
                'where': where,
                'cursor': cursor,
            },
            root_selection=root_selection,
        )

        if select is None:
            return cast(int, resp['data']['result']['_count']['_all'])
        else:
            return cast(types.MCountAggregateOutput, resp['data']['result']['_count'])

